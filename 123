createRowButtons({
	
-- bi·∫øn l∆∞u r∆∞∆°ng ƒë√£ teleport l·∫ßn tr∆∞·ªõc
	local lastChest = nil
	local minDistance = 0

	{name = "Chest 1", action = function()
		local player = game.Players.LocalPlayer
		local char = player.Character or player.CharacterAdded:Wait()
		local hrp = char:WaitForChild("HumanoidRootPart")
		local lastChest = nil
		local minDistance = 0
		local nearest, distance = nil, math.huge

		for _, item in ipairs(workspace.Items:GetChildren()) do
			if item.Name == "Item Chest" then
				local capPart = item:FindFirstChild("ItemDrop", true)
				if capPart and capPart:IsA("BasePart") then
					local dist = (capPart.Position - hrp.Position).Magnitude
					
					-- b·ªè qua n·∫øu qu√° g·∫ßn ho·∫∑c nh·ªè h∆°n kho·∫£ng c√°ch t·ªëi thi·ªÉu
					if lastChest and (capPart.Position - lastChest.Position).Magnitude < minDistance then
						continue
					end

					if dist < distance then
						nearest, distance = capPart, dist
					end
				end
			end
		end

		if nearest then
			lastChest = nearest
			minDistance = minDistance + 10000 -- tƒÉng kho·∫£ng c√°ch m·ªói l·∫ßn
			hrp.CFrame = nearest.CFrame + Vector3.new(0, 5, 0)
			warn("ƒê·∫øn chest m·ªõi, minDistance:", minDistance)
		end
	end}


